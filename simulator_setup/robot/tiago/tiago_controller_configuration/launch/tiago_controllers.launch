<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="arm"  default="True"/>
    <arg name="end_effector"  default="pal-hey5"/>
    <arg name="ft_sensor"  default="schunk-ft"/>
    <arg name="laser_model"  default="sick-571"/>
    <arg name="camera_model"  default="orbbec-astra"/>
    <arg name="wrist_model"  default="wrist-2010"/>
    <arg name="base_type_suffix" default=""/>

    <include file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/launch/joint_state_controller.launch"/>
    <include file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/launch/default_controllers.launch">
        <arg name="arm" value="$(arg arm)"/>
        <arg name="base_type_suffix"  value="$(arg base_type_suffix)"/>
    </include>
    <group if="$(eval ft_sensor == 'schunk-ft')">
      <include file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/launch/force_torque_controller.launch"/>
    </group>
    <!-- load gravity compensation controller with controllable wrist -->
    <include if="$(arg arm)" file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/launch/gravity_compensation.launch">
      <arg name="end_effector" value="$(arg end_effector)"/>
      <arg name="wrist_model"  value="$(arg wrist_model)"/>
    </include>

    <group if="$(eval end_effector == 'pal-gripper')">
        <rosparam command="load" file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/config/pal-gripper_joint_trajectory_controllers.yaml" />
        <include file="$(find pal_gripper_controller_configuration)/launch/gripper_controller.launch"/>
    </group>

    <group if="$(eval end_effector == 'robotiq-2f-85')">
        <rosparam command="load" file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/config/robotiq-2f-85_joint_trajectory_controllers.yaml" />
        <include file="$(find pal_robotiq_controller_configuration)/launch/gripper_controller.launch">
          <arg name="model" value="$(arg end_effector)" />
        </include>
    </group>

    <group if="$(eval end_effector == 'robotiq-2f-140')">
        <rosparam command="load" file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/config/robotiq-2f-140_joint_trajectory_controllers.yaml" />
        <include file="$(find pal_robotiq_controller_configuration)/launch/gripper_controller.launch">
          <arg name="model" value="$(arg end_effector)" />
        </include>
    </group>

    <group if="$(eval end_effector == 'schunk-wsg')">
        <rosparam command="load" file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/config/schunk-wsg_joint_trajectory_controllers.yaml" />
        <include file="$(find pal_wsg_gripper_controller_configuration)/launch/gripper_controller.launch"/>
        <include file="$(find pal_wsg_gripper_controller_configuration)/launch/gripper_fingertip_sensor_controller.launch">
          <arg name="finger" value="left"/>
        </include>
        <include file="$(find pal_wsg_gripper_controller_configuration)/launch/gripper_fingertip_sensor_controller.launch">
          <arg name="finger" value="right"/>
        </include>
    </group>
    <group if="$(eval end_effector == 'pal-hey5')">
        <rosparam command="load" file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/config/pal-hey5_joint_trajectory_controllers.yaml" />
        <include file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/launch/hey5_controller.launch"/>
    </group>

    <!-- Only loads the impedance controllers -->
    <include file="$(find simulator_setup)/robot/tiago/tiago_controller_configuration/launch/impedance_controller.launch">
      <arg name="end_effector" value="$(arg end_effector)"/>
      <arg name="wrist_model"  value="$(arg wrist_model)"/>
    </include>

    <!-- Motion planning -->
    <include file="$(find simulator_setup)/robot/tiago/tiago_moveit_config/launch/move_group.launch">
      <arg name="arm" value="$(arg arm)"/>
      <arg name="end_effector" value="$(arg end_effector)"/>
      <arg name="ft_sensor" value="$(arg ft_sensor)"/>
      <arg name="laser_model" value="$(arg laser_model)"/>
      <arg name="camera_model" value="$(arg camera_model)"/>
    </include>
</launch>
